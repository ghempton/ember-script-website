    <div class='main'>
      // Generated by EmberScript 0.0.14
      void function () {
        var PostsController;
        var get$ = Ember.get;
        PostsController = Ember.ArrayController.extend({
          trimmedPosts: Ember.computed(function () {
            if (null != get$(this, 'content'))
              return get$(this, 'content').slice(0, 3);
          }).property('content.@each'),
          postsChanged: Ember.observer(function () {
            return console.log('changed');
          }, 'content.@each')
        });
      }.call(this);
    </div>
